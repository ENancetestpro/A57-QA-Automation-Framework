<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Regression Suite">
    <parameter name="BaseURL" value="https://qa.koel.app/"/>
    <test name="Regression Test" preserve-order="false">
        <classes>
            <class name="LoginTests">
                <methods>
                    <include name="invalidEmailValidPasswordTest"/>
                    <include name="validEmailInvalidPasswordTest"/>
                    <include name="emptyEmailPasswordTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
